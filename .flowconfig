[ignore]
.*/node_modules/*
.*/build/*
.*/dist/*
.*/doc/*
.*/mock/*
.*/scripts/*
.*/test/*

[include]
./src/js

[libs]
./flow.js
flow-typed/


[options]
module.system=haste
module.file_ext=.js
module.file_ext=.json
module.file_ext=.jsx
module.file_ext=.css
module.file_ext=.scss
module.name_mapper='^jscom\/\(.*\)$' -> '<PROJECT_ROOT>/src/\1'
esproposal.class_instance_fields=enable
esproposal.class_static_fields=enable
esproposal.decorators=ignore
