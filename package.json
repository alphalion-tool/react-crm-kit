{
  "name": "front",
  "version": "1.0.0",
  "description": "front for bluesea",
  "main": "index.js",
  "scripts": {
    "analysis": "node_modules/.bin/webpack --json | webpack-bundle-size-analyzer",
    "debug": "node --nolazy --inspect-brk=9229 server/server.js",
    "flow": "node_modules/.bin/flow",
    "flow-coverage": "node_modules/.bin/flow-coverage-report",
    "lint-staged": "node_modules/.bin/lint-staged",
    "babel-node": "node_modules/.bin/babel-node",
    "test": "export NODE_ENV=test && node_modules/.bin/nodemon --exec npm run babel-node -- ./scripts/jasmine_test.js",
    "coverage": "sh shells/test.sh",
    "build": "export NODE_ENV=production && NODE_DEBUG=fs && node_modules/.bin/webpack --config ./webpack.build.js",
    "dll": "node_modules/.bin/webpack --config ./webpack.dll.config.js",
    "dev": "node_modules/.bin/babel-node --max_old_space_size=4096 webpack-dev-server/server.js",
    "server": "node_modules/.bin/babel-node server/server.js online",
    "lint": "node_modules/.bin/eslint --ext \".js\" src/",
    "lintquiet": "node_modules/.bin/eslint --ext \".js\" src/  --quiet",
    "webpack": "node_modules/.bin/webpack -w",
    "jsdoc": "node_modules/.bin/jsdoc -c doc/jsdoc.json -r",
    "stylelint": "node_modules/.bin/stylelint 'src/**/*.scss' --syntax scss",
    "lintgit": "node scripts/git_status_eslint",
    "stylelintgit": "node scripts/git_status_stylelint",
    "csslintgit": "node scripts/git_status_stylelint"
  },
  "keywords": [
    "bluesea"
  ],
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "node_modules/.bin/eslint --ignore-path .eslintignore"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node_modules/.bin/validate-commit-msg"
    }
  },
  "author": "deju",
  "license": "ISC",
